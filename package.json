{
  "name": "Socialize-starter",
  "version": "1.0.0",
  "description": "Meteor starter application utilizing Socialize packages.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "./node_modules/.bin/eslint ./lib ./client ./server ./intl --ext .js,.jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001",
    "test-storybook": "storyshots"
  },
  "author": "Jan Dvorak",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "mantra-core": "^1.7.0",
    "meteor-node-stubs": "^0.2.5",
    "prop-types": "^15.5.8",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.2.3",
    "react-router": "^3.0.2",
    "react-simple-di": "^1.2.0",
    "sanitize-html": "^1.14.1",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.x.x",
    "babel-core": "^6.x.x",
    "babel-eslint": "^7.1.1",
    "babel-plugin-react-require": "^3.0.0",
    "babel-polyfill": "^6.x.x",
    "babel-preset-es2015": "^6.x.x",
    "babel-preset-meteor": "^6.x.x",
    "babel-preset-react": "^6.x.x",
    "babel-preset-stage-2": "^6.x.x",
    "babel-root-slash-import": "^1.x.x",
    "babel-runtime": "^6.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.4.0",
    "eslint": "^3.5.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.x.x",
    "eslint-plugin-react": "^6.2.2",
    "g": "^2.0.1",
    "jsdom": "^10.0.0",
    "mantra-lint": "^0.0.1",
    "mocha": "3.x.x",
    "proxyquire": "^1.7.10",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.x",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1"
  }
}
